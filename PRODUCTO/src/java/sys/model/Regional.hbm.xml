<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class  name="sys.model.Regional" optimistic-lock="version" table="REGIONAL" schema="dbo" catalog="PRODUCTO">
          <id name="codZona" type="string">
            <column name="CODZONA" />
            <generator class="assigned" />
        </id>
        <property name="nombre" type="string">
            <column name="NOMBRE"   />
        </property>
        
  </class>    
</hibernate-mapping>
